<div class="container align-items-center justify-content-center">
    <div class = "text-center row">
        <h2>Highscores</h2>
    </div>
    <div class = "gamer-font column">
        <div class = "m-auto text-center w-25 h4 p-4 neon-border">YOU HAVE {{totalScoreData}} KILLS!</div>
        <br>
        <div class = "m-auto p-4 h4 text-center justify-center w-25 fit neon-border"> Your history:
            {{#each highScores as |highScore| }}
                <div>{{highScore.score}}</div>
                <br>
            {{/each}}
        </div>
    </div>
<script src="/js/profile.js"></script>