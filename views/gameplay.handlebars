<div class="container vw-100 mt-4">
    <div class = "row w-100" id="gameScore">
        <div class = 'col-12'>
        <h4>Leaderboard</h4>
        <div id="playerScore"></div>
        </div>
    </div>
</div>
<div class = 'container' id="canvasHolder">
    {{#if loggedIn}}
    <div class = "my-5 row align-items-end justify-content-center">
    <canvas id="canvas1"></canvas>
    </div>
    {{else}}
    <h2 id="login-status">Must be logged in, in order to play!</h2>
    <style>
        #gameScore,
        #canvas1 {
            display: none
        }
    </style>
    {{/if}}
</div>


